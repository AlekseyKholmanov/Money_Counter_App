<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/main_graph"
    app:startDestination="@id/keyboardFragment">

    <fragment
        android:id="@+id/keyboardFragment"
        android:name="com.example.holmi_production.money_counter_app.ui.fragments.KeyboardFragment"
        android:label="KeyboardFragment" >
        <action
            android:id="@+id/action_keyboardFragment_to_selectCategoryFragment"
            app:destination="@id/selectCategoryFragment" />
        <action
            android:id="@+id/action_keyboardFragment_to_bottomKeyboard"
            app:destination="@id/bottomKeyboard" />
    </fragment>
    <fragment
        android:id="@+id/chartFragment"
        android:name="com.example.holmi_production.money_counter_app.ui.fragments.charts.ChartFragment"
        android:label="ChartFragment" />
    <fragment
        android:id="@+id/costsFragment"
        android:name="com.example.holmi_production.money_counter_app.ui.fragments.TransactionFragment"
        android:label="CostsFragment" />
    <fragment
        android:id="@+id/selectCategoryFragment"
        android:name="com.example.holmi_production.money_counter_app.ui.fragments.SelectCategoryFragment"
        android:label="SelectCategoryFragment" >
        <action
            android:id="@+id/action_selectCategoryFragment_to_createCategoryFragment"
            app:destination="@id/createCategoryFragment" />
    </fragment>
    <fragment
        android:id="@+id/limitsFragment"
        android:name="com.example.holmi_production.money_counter_app.ui.fragments.LimitsFragment"
        android:label="LimitsFragment" />
    <fragment
        android:id="@+id/createCategoryFragment"
        android:name="com.example.holmi_production.money_counter_app.ui.fragments.CreateCategoryFragment"
        android:label="CreateCategoryFragment" />
    <dialog
        android:id="@+id/bottomKeyboard"
        android:name="com.example.holmi_production.money_counter_app.ui.dialogs.BottomKeyboard"
        android:label="BottomKeyboard" />
</navigation>