<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
            xmlns:app="http://schemas.android.com/apk/res-auto"
            xmlns:tools="http://schemas.android.com/tools"
            android:id="@+id/nav_graph_main"
            app:startDestination="@id/navMain">
    <fragment
            android:id="@+id/navMain"
            android:name="com.example.holmi_production.money_counter_app.BottomHostFragment"
            android:label=""
            tools:layout="@layout/fragment_bottom_container"/>
    <fragment
            android:id="@+id/navFirstLaunch"
            android:name="com.example.holmi_production.money_counter_app.ui.first_launch_fragment.FirstLaunchFragment"
            android:label="Home"
            tools:layout="@layout/fragment_first_launch">
        <action android:id="@+id/action_navFirstLaunch_to_navMain" app:destination="@id/navMain"
                app:launchSingleTop="false" app:enterAnim="@anim/nav_default_enter_anim"
                app:exitAnim="@anim/nav_default_enter_anim" app:popUpToInclusive="false"
                app:popUpTo="@+id/nav_graph_main"/>
    </fragment>
    <fragment
            android:id="@+id/navEndPeriod"
            android:name="com.example.holmi_production.money_counter_app.ui.end_period_fragment.EndPeriodFragment"
            android:label=""
            tools:layout="@layout/fragment_end_period">
        <action android:id="@+id/action_navEndPeriod_to_navMain" app:destination="@id/navMain"
                app:enterAnim="@anim/nav_default_enter_anim" app:exitAnim="@anim/nav_default_exit_anim"
                app:popUpToInclusive="false" app:launchSingleTop="false" app:popUpTo="@+id/nav_graph_main"/>
    </fragment>

</navigation>